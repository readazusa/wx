<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="club.lovety.user.entity.UserParticle">
    <insert id="save" parameterType="UserParticle">
        insert  into t_wx_userparticle(uid,openid,nick,realname,latitude,longitude,precisions,createTime)
        values(#{uid},#{openId},#{nick},#{realName},#{latitude},#{longitude},#{precisions},#{createTime})
    </insert>


</mapper>
