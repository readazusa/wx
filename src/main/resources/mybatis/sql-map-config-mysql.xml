<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
        <setting name="lazyLoadingEnabled" value="true"/>  
        <setting name="aggressiveLazyLoading" value="false"/>

	</settings>

	<typeAliases>
		<typeAlias type="club.lovety.item.entity.FileInfo" alias="FileInfo"></typeAlias>
        <typeAlias type="club.lovety.item.entity.ItemInfo" alias="ItemInfo"></typeAlias>
		<typeAlias type="club.lovety.item.entity.ItemTypeInfo" alias="ItemTypeInfo"></typeAlias>
		<typeAlias type="club.lovety.portal.entity.IndexPageInfo" alias="IndexPageInfo"></typeAlias>

		<typeAlias type="club.lovety.base.entity.BaseSearchPO" alias="BaseSearchPO"></typeAlias>
		<typeAlias type="club.lovety.shopcart.po.ShopCartInfo" alias="ShopCartInfo"></typeAlias>

		<typeAlias type="club.lovety.user.entity.AddressInfo" alias="AddressInfo"></typeAlias>
		<typeAlias type="club.lovety.user.entity.UserInfo" alias="UserInfo"></typeAlias>
		<typeAlias type="club.lovety.user.entity.UserParticle" alias="UserParticle"></typeAlias>
		<typeAlias type="club.lovety.user.entity.UserVsAddressInfo" alias="UserVsAddressInfo"></typeAlias>


		<typeAlias type="club.lovety.yj.entity.YjInfo" alias="YjInfo"></typeAlias>
		<!--<typeAlias type="club.lovety.base.entity.BasePagePO" alias="BasePagePO"></typeAlias>-->
	</typeAliases>

	<mappers>
		<mapper resource="club/lovety/item/entity/mybatis/FileInfo.xml"></mapper>
		<mapper resource="club/lovety/item/entity/mybatis/ItemInfo.xml"></mapper>
		<mapper resource="club/lovety/item/entity/mybatis/ItemTypeInfo.xml"></mapper>
		<mapper resource="club/lovety/portal/entity/mybatis/IndexPageInfo.xml"></mapper>
		<mapper resource="club/lovety/shopcart/po/mybatis/ShopCartInfo.xml"></mapper>

		<mapper resource="club/lovety/user/entity/mybatis/AddressInfo.xml"></mapper>

		<mapper resource="club/lovety/user/entity/mybatis/UserInfo.xml"></mapper>
		<mapper resource="club/lovety/user/entity/mybatis/UserParticle.xml"></mapper>
		<mapper resource="club/lovety/yj/entity/mybatis/YjInfo.xml"></mapper>

	</mappers>

</configuration>
 
